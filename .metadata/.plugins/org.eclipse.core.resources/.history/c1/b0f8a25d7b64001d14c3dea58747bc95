package poo;

public class Carro {

	int ano;
	String cor;
	
	public Carro () {
		System.out.println("Chassi: ");
	}
	
	void ligar() {
		System.out.println("engine ON........");
	}
	void desligar() {
		System.out.println("engine OFF........");
	}
	void acelerar() {
		System.out.println("vrummmmmm........");
	
	
		
	}
}
